package TrabalhoPratico8;

public class Funcionario
{
    
    private  String nome;
    private int id;
    
    
    public  String  getNome () {
        return nome;
    }
    public  void  setNome ( String  nome ) {
        this.nome = nome;
    }
    public  int  getId () {
        return id;
    }
    public  void  setMatricula ( int  matricula ) {
        this.id = id;
    }
    
    
}
